#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'vsg==3.12.0.dev1','console_scripts','vsg'
__requires__ = 'vsg==3.12.0.dev1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vsg==3.12.0.dev1', 'console_scripts', 'vsg')()
    )
